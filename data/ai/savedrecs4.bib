@article{ ISI:000330158700002,
Author = {Irani, Zahir and Kamal, Muhammad Mustafa},
Title = {{Intelligent Systems Research in the Construction Industry}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
