@article{ ISI:000377230300002,
Author = {Zhang, Chun-Yang and Chen, C. L. Philip and Chen, Dewang and Ng, Kin Tek},
Title = {{MapReduce based distributed learning algorithm for Restricted Boltzmann
   Machine}},
Journal = {{NEUROCOMPUTING}},
Year = {{2016}},
Volume = {{198}},
