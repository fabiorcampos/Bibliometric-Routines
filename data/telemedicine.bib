@article{ ISI:000419036700012,
Author = {Clark, M. P. A.},
Title = {{Endoscopic ear surgery in the ear camp setting; forward thinking or
   folly?}},
Journal = {{JOURNAL OF LARYNGOLOGY AND OTOLOGY}},
Year = {{2018}},
Volume = {{132}},
Number = {{1}},
Pages = {{68-70}},
Month = {{JAN}},
Abstract = {{Background: Endoscopic ear surgery is becoming an accepted technique in
   otological surgery, in the management of chronic otitis media.
   Methods: The technique was introduced to the humanitarian care setting
   of an ear camp in Nepal to consider the appropriateness of the technique
   in this type of clinical setting.
   Results: Fifteen cases of myringoplasty were successfully completed.
   Conclusion: The apparent advantages of the endoscopic approach over the
   traditional microscopic one were the ease of transporting the equipment
   and the optimal view obtained of the tympanic membrane. The ability for
   pathology and operative technique to be observed equally well by the
   surgeon and local staff was helpful for teaching, and enables a move
   towards self-sufficiency of care. Easy image capture also has potential
   for remote telemedicine applications.}},
