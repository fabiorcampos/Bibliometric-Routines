@article{ ISI:000326556300010,
Author = {Recio-Garcia, Juan A. and Gonzalez-Calero, Pedro A. and Diaz-Agudo,
   Belen},
Title = {{jCOLIBRI2: A framework for building Case-based reasoning systems}},
Journal = {{SCIENCE OF COMPUTER PROGRAMMING}},
Year = {{2014}},
Volume = {{79}},
Number = {{SI}},
Pages = {{126-145}},
Month = {{JAN 1}},
Abstract = {{This paper describes the jCOLIBRI2 framework for building Case-based
   reasoning (CBR) systems. CBR is a mature subfield of artificial
   intelligence based on the reuse of previous problem solutions - cases -
   to solve new ones. However, up until now, it lacked a reference toolkit
