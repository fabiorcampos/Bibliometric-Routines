@article{ ISI:000387531100008,
Author = {Keshta, H. E. and Ali, A. A. and Saied, E. M. and Bendary, F. M.},
Title = {{Application of Static Var Compensator (SVC) With PI Controller for Grid
   Integration of Wind Farm Using Harmony Search}},
