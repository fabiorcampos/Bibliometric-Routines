@article{ ISI:000366879800138,
Author = {Barbosa Nogueira, Yuri Lenon and Fisch de Brito, Carlos Eduardo and
   Vidal, Creto Augusto and Cavalcante Neto, Joaquim Bento},
Title = {{Emergent vision system of autonomous virtual characters}},
Journal = {{NEUROCOMPUTING}},
Year = {{2016}},
Volume = {{173}},
Pages = {{1851-1867}},
Month = {{JAN 15}},
Abstract = {{This paper addresses the problem of autonomous generation of sensory
   systems of autonomous virtual characters. We believe, following the new
   trends of Artificial Intelligence, that the level of coherence required
   for natural behavior of a virtual character in complex worlds can only
   be achieved by a strong connection between its mind and the surrounding
   environment. Therefore, the characters' sensory system plays a
   fundamental role on that relationship. Vision is particularly important
   because it allows the agent to perceive its surroundings without the
   help of any external description. Thus, in order to evolve an artificial
   vision system for a virtual character, we devised a novel technique for
   genetically encoding artificial neural networks. Through our technique,
   we are able to obtain emergent functional controllers by structurally
   coupling the neural network to the environment. Two types of evolution
   were tested: an objective-guided evolution; and an environment-guided
   evolution, for which no descriptions of objectives are provided. (C)
   2015 Elsevier B.V. All rights reserved.}},
