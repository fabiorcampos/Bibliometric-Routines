@article{ ISI:000367878800001,
Author = {Albayati, Mohanad and Issac, Biju},
Title = {{Analysis of Intelligent Classifiers and Enhancing the Detection Accuracy
   for Intrusion Detection System}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL INTELLIGENCE SYSTEMS}},
Year = {{2015}},
Volume = {{8}},
Number = {{5}},
Pages = {{841-853}},
Abstract = {{In this paper we discuss and analyze some of the intelligent classifiers
   which allows for automatic detection and classification of networks
   attacks for any intrusion detection system. We will proceed initially
   with their analysis using the WEKA software to work with the classifiers
   on a well-known IDS (Intrusion Detection Systems) dataset like NSL-KDD
   dataset. The NSL-KDD dataset of network attacks was created in a
   military network by MIT Lincoln Labs. Then we will discuss and
