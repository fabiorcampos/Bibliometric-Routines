@article{ ISI:000376301200008,
Author = {Talbi, El-Ghazali},
Title = {{Combining metaheuristics with mathematical programming, constraint
   programming and machine learning}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
Year = {{2016}},
Volume = {{240}},
Number = {{1}},
Pages = {{171-215}},
Month = {{MAY}},
Abstract = {{During the last years, interest on hybrid metaheuristics has risen
   considerably in the field of optimization and machine learning. The best
   results found for many optimization problems in science and industry are
   obtained by hybrid optimization algorithms. Combinations of optimization
   tools such as metaheuristics, mathematical programming, constraint
   programming and machine learning, have provided very efficient
   optimization algorithms. Four different types of combinations are
   considered in this paper: (1) Combining metaheuristics with
   complementary metaheuristics. (2) Combining metaheuristics with exact
   methods from mathematical programming approaches which are mostly used
   in the operations research community. (3) Combining metaheuristics with
   constraint programming approaches developed in the artificial
   intelligence community. (4) Combining metaheuristics with machine
   learning and data mining techniques.}},
