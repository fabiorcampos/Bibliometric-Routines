@article{ ISI:000325745600033,
Author = {Lewandowski, Jacek and Rybinski, Henryk},
Title = {{Generalized Hybrid Encoding of Polyhierarchical Structures}},
Journal = {{FUNDAMENTA INFORMATICAE}},
Year = {{2013}},
Volume = {{127}},
Number = {{1-4}},
Pages = {{461-477}},
Abstract = {{Polyhierarchical structures play an important role in artificial
   intelligence, especially in knowledge representation. The main problem
   with using them efficiently is lack of efficient methods of accessing
   related nodes, which limits the practical applications. The proposed
   hybrid indexing approach generalizes various methods and makes possible
   combining them in a uniform manner within one index, which adapts to a
   particular topology of the data structure. This gives rise to a balance
   between compactness of the index and fast responses to the search
   requests. The correctness of the proposed method is formally shown, and
   its performance is evaluated. The results prove its high efficiency.}},
