@article{ ISI:000350664700030,
Author = {Lujic, Roberto and Samardzic, Ivan and Galzina, Vjekoslav},
Title = {{APPLICATION OF EXPERT SYSTEMS FOR SELECTION OF INSTALLATION PIPES}},
Journal = {{TEHNICKI VJESNIK-TECHNICAL GAZETTE}},
Year = {{2015}},
Volume = {{22}},
Number = {{1}},
Pages = {{241-245}},
Month = {{FEB}},
Abstract = {{The enterprises want to minimise the overall production costs, to
   maintain the quality and to increase the competitiveness on the market.
   It is possible to increase the number of customers, efficiency and
   effectiveness through the product quality. Therefore, every enterprise
   needs to have a good organisational structure, good experts and the
   knowledge how to do qualitative work. The paper shows the application of
   expert system to the problem of selection of installation pipes
   according to the given attributes. The input attributes are: mechanical
   load, material, colour, flame resistance, length, packaging, temperature
   range and outer diameter. The output parameter is the type of
