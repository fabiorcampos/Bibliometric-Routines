@article{ ISI:000411111400009,
Author = {Gonzalez-Pardo, Antonio and Del Ser, Javier and Camacho, David},
Title = {{Solving strategy board games using a CSP-based ACO approach}},
Journal = {{INTERNATIONAL JOURNAL OF BIO-INSPIRED COMPUTATION}},
Year = {{2017}},
Volume = {{10}},
Number = {{2, SI}},
Pages = {{136-144}},
Abstract = {{In the last years, there have been a huge increase in the number of
   research contributions that use games and video-games as an application
   domain for testing different artificial intelligence algorithms. Some of
   these problems can be represented as a constraint satisfaction problem
   (CSP), and heuristics algorithms (such as ant colony optimisation) can
   be used due to the complexity of the modelled problems. This paper
   presents a comparative study of the performance of a novel ACO model for
   CSP-based board games. In this work, two different oblivion rate
   meta-heuristics for controlling the number of pheromones created in the
   model have been created. Experimental results reveal that both
   meta-heuristics reduce considerably the number of pheromones produced in
   the system without affecting the quality of the solutions in terms of
   average optimality.}},
