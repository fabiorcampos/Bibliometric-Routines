@article{ ISI:000328732600005,
Author = {Kao, Kuo-Yuan and Wu, I-Chen and Yen, Shi-Jim and Shan, Yi-Chang},
Title = {{Incentive Learning in Monte Carlo Tree Search}},
Journal = {{IEEE TRANSACTIONS ON COMPUTATIONAL INTELLIGENCE AND AI IN GAMES}},
Year = {{2013}},
Volume = {{5}},
Number = {{4}},
Pages = {{346-352}},
Month = {{DEC}},
Abstract = {{Monte Carlo tree search (MCTS) is a search paradigm that has been
   remarkably successful in computer games like Go. It uses Monte Carlo
   simulation to evaluate the values of nodes in a search tree. The node
   values are then used to select the actions during subsequent
   simulations. The performance of MCTS heavily depends on the quality of
   its default policy, which guides the simulations beyond the search tree.
   In this paper, we propose an MCTS improvement, called incentive
   learning, which learns the default policy online. This new default
   policy learning scheme is based on ideas from combinatorial game theory,
   and hence is particularly useful when the underlying game is a sum of
   games. To illustrate the efficiency of incentive learning, we describe a
   game named Heap-Go and present experimental results on the game.}},
