@article{ ISI:000343840000023,
Author = {Tungadio, D. H. and Numbi, B. P. and Siti, M. W. and Jimoh, A. A.},
Title = {{Particle swarm optimization for power system state estimation}},
Journal = {{NEUROCOMPUTING}},
Year = {{2015}},
