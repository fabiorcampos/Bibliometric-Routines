@article{ ISI:000385716100004,
Author = {Corcoba Magana, Victor and Munoz-Organero, Mario},
Title = {{Artemisa: A Personal Driving Assistant for Fuel Saving}},
Journal = {{IEEE TRANSACTIONS ON MOBILE COMPUTING}},
Year = {{2016}},
Volume = {{15}},
Number = {{10}},
Pages = {{2437-2451}},
Month = {{OCT 1}},
Abstract = {{In this paper, we propose a driving assistant that makes recommendations
   in order to reduce the fuel consumption. The solution only requires a
   smartphone and an OBD/Bluetooth device. Eco-driving advices try to avoid
   situations that cause an increase in the fuel consumption such as
   inappropriate speed or slow reaction to the detection of traffic signs
   and traffic incidents. The main contribution of this paper is the use of
   artificial intelligence techniques in order to issue the eco-driving
   tips that are best adapted to the user profile, the characteristics of
   the vehicle, and the road state conditions. This is very important
   because the driver may lose the interest due to the high requirements
   that tend to be provided by general use eco-driving assistants. In order
