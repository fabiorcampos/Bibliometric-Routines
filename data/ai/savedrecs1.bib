@article{ ISI:000385246200007,
Author = {Herrero, Alvaro and Saiz-Barcena, Lourdes and Manzanedo, Miguel A. and
   Corchado, Emilio},
Title = {{A hybrid proposal for cross-sectoral analysis of knowledge management}},
Journal = {{SOFT COMPUTING}},
Year = {{2016}},
Volume = {{20}},
Number = {{11, SI}},
Pages = {{4271-4285}},
Month = {{NOV}},
Abstract = {{At present time, although many theoretical formulations have been
   successfully proposed, there is a lack of ICT-based tools to support
   practical deployment of knowledge management (KM) in real settings. To
   bridge this gap, a hybrid artificial intelligence system is proposed in
   present study, aimed at gaining deeper knowledge about KM practices in
   four different economic sectors. By means of soft computing, companies
   are diagnosed according to their status regarding KM and subsequent
   explanations about crucial KM practices and perspectives are generated.
   Interesting conclusions are then derived from these explanations,
   allowing KM managers to optimise their decisions and obtain better
   results. Experimental results of real-life data from Spanish companies
   associated with different economic sectors validate the proposed
   combination of techniques.}},
