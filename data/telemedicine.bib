@article{ ISI:000413898400006,
Author = {Wade, Victoria and Barnett, Adrian G. and Martin-Khan, Melinda and
   Russell, Trevor},
Title = {{Designing quantitative telemedicine research}},
Journal = {{JOURNAL OF TELEMEDICINE AND TELECARE}},
Year = {{2017}},
Volume = {{23}},
Number = {{9}},
Pages = {{786-791}},
Month = {{OCT}},
Abstract = {{When designing quantitative trials and evaluation of telehealth
   interventions, researchers should think ahead to the intended way that
   the intervention could be implemented in routine care and consider how
   trial participants with similar characteristics to the target population
   can be included. The telehealth intervention and the context in which it
   is placed should be clearly described, and consideration given to
   conducting pragmatic trials in order to show the effect of telehealth in
   complex environments with rapidly changing technology. Types of research
   designs, comparators and outcome measures are discussed and common
   statistical issues are introduced.}},
