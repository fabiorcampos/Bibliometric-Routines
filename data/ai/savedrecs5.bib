@article{ ISI:000325759200018,
Author = {Sundar, Shyam and Singh, Alok},
Title = {{New heuristic approaches for the dominating tree problem}},
Journal = {{APPLIED SOFT COMPUTING}},
Year = {{2013}},
Volume = {{13}},
Number = {{12}},
Pages = {{4695-4703}},
Month = {{DEC}},
Abstract = {{Given an undirected, connected and edge-weighted graph, the dominating
   tree problem (DTP) seeks on this graph a tree with minimum total edge
   weight such that each vertex of the graph is either in this tree or
   adjacent to a vertex in this tree. The DTP is a.V2-Hard problem. In the
   literature only two heuristics for this problem are proposed so far in
   spite of the fact that it has several practical applications in the
   field of wireless sensor networks. In this paper, we propose one
   heuristic and two swarm intelligence techniques, viz, an artificial bee
   colony algorithm and an ant colony optimization algorithm for the DTP.
   Computational results show the effectiveness of our approaches. (C) 2013
   Elsevier B.V. All rights reserved.}},
