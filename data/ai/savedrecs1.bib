@article{ ISI:000388053700005,
Author = {Sergio, Anderson T. and de Lima, Tiago P. F. and Ludermir, Teresa B.},
Title = {{Dynamic selection of forecast combiners}},
Journal = {{NEUROCOMPUTING}},
Year = {{2016}},
Volume = {{218}},
Pages = {{37-50}},
Month = {{DEC 19}},
Abstract = {{Time series forecasting is an important research field in machine
   learning. Since the literature shows several techniques for the solution
   of this problem, combining outputs of different models is a simple and
   robust strategy. However, even when using combiners, the experimenter
   may face the following dilemma: which technique should one use to
   combine the individual predictors? Inspired by classification and
   pattern recognition algorithms, this work presents a dynamic selection
   method of forecast combiners. In the dynamic selection, each test
   pattern is submitted to a certain combiner according to a nearest
   neighbor rule. The proposed method was used to forecast eight time
   series with chaotic behavior in short and long term. In general, the
   dynamic selection presented satisfactory results for all datasets. (C)
   2016 Elsevier B.V. All rights reserved.}},
