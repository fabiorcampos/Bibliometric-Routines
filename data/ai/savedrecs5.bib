@article{ ISI:000315062600004,
Author = {Ireland, Andrew and Grov, Gudmund and Llano, Maria Teresa and Butler,
   Michael},
Title = {{Reasoned modelling critics: Turning failed proofs into modelling
   guidance}},
Journal = {{SCIENCE OF COMPUTER PROGRAMMING}},
Year = {{2013}},
Volume = {{78}},
Number = {{3, SI}},
Pages = {{293-309}},
Month = {{MAR 1}},
Note = {{2nd International Conference on Abstract State Machines (ASM), Alloy, B
   and Z, Orford, CANADA, FEB 22-25, 2010}},
Organization = {{Univ Sherbrooke; Univ Paris Est Creteil}},
Abstract = {{The activities of formal modelling and reasoning are closely related.
   But while the rigour of building formal models brings significant
   benefits, formal reasoning remains a major barrier to the wider
   acceptance of formalism within design. Here we propose reasoned
   modelling critics - an approach which aims to abstract away from the
   complexities of low-level proof obligations, and provide high-level
   modelling guidance to designers when proofs fail. Inspired by proof
   planning critics, the technique combines proof-failure analysis with
   modelling heuristics. Here, we present the details of our proposal,
   implement them in a prototype and outline future plans. (c) 2011
   Elsevier B.V. All rights reserved.}},
