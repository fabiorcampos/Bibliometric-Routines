@article{ ISI:000424337100048,
Author = {Logan, Robert K.},
Title = {{Can Computers Become Conscious, an Essential Condition for the
   Singularity?}},
Journal = {{INFORMATION}},
Year = {{2017}},
Volume = {{8}},
Number = {{4}},
Month = {{DEC}},
Abstract = {{Given that consciousness is an essential ingredient for achieving
   Singularity, the notion that an Artificial General Intelligence device
   can exceed the intelligence of a human, namely, the question of whether
   a computer can achieve consciousness, is explored. Given that
   consciousness is being aware of one's perceptions and/or of one's
   thoughts, it is claimed that computers cannot experience consciousness.
