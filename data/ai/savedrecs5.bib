Author = {Jimenez-Come, M. J. and Munoz, E. and Garcia, R. and Matres, V. and
   Martin, M. L. and Trujillo, F. and Turias, I.},
Title = {{Pitting corrosion behaviour of austenitic stainless steel using
   artificial intelligence techniques}},
