@article{ ISI:000313155700023,
Author = {Singh, Th Suka Deba and Chatterjee, Amitava},
Title = {{MMSE design of nonlinear Volterra equalizers using artificial bee colony
   algorithm}},
Journal = {{MEASUREMENT}},
Year = {{2013}},
Volume = {{46}},
Number = {{1}},
Pages = {{210-219}},
Month = {{JAN}},
Abstract = {{In this paper a novel approach for channel equalization is presented,
   where a framework for Volterra system is used to model both the channel
   and the equalizer. We propose development of first-order and
   second-order Volterra equalizers using minimum mean square error (MMSE)
   approach and design these equalizers using swarm intelligence based
   stochastic optimization algorithm which is applied to adapt the
   equalizer coefficients to the time varying channel. This work proposes
   to use the artificial bee colony (ABC) algorithm, recently introduced
   for global optimization, simulating the intelligent foraging behavior of
   honey bee swarm in a simple, robust, and flexible manner. For
   comparative analysis, adaptive equalizers like least mean squares (LMSs)
   equalizer, recursive least squares (RLSs) equalizer and least mean
   p-Norm (LMP) equalizer and population based optimum equalizers employing
   PSO are also applied for identical problems and the superiority of the
   newly proposed algorithm is aptly demonstrated. (C) 2012 Elsevier Ltd.
