@article{ ISI:000412448200003,
Author = {Larson, C. E. and Van Cleemput, N.},
Title = {{Automated conjecturing III}},
Journal = {{ANNALS OF MATHEMATICS AND ARTIFICIAL INTELLIGENCE}},
Year = {{2017}},
Volume = {{81}},
Number = {{3-4}},
Pages = {{315-327}},
Month = {{DEC}},
Abstract = {{Discovery in mathematics is a prototypical intelligent behavior, and an
   early and continuing goal of artificial intelligence research. We
   present a heuristic for producing mathematical conjectures of a certain
   typical form and demonstrate its utility. Our program conjectures
   relations that hold between properties of objects (property-relation
   conjectures). These objects can be of a wide variety of types. The
   statements are true for all objects known to the program, and are the
   simplest statements which are true of all these objects. The examples
   here include new conjectures for the hamiltonicity of a graph, a
   well-studied property of graphs. While our motivation and experiments
   have been to produce mathematical conjectures-and to contribute to
   mathematical research-other kinds of interesting property-relation
   conjectures can be imagined, and this research may be more generally
   applicable to the development of intelligent machinery.}},
