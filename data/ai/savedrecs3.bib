@article{ ISI:000364883900138,
Author = {Wong, Shen Yuong and Yap, Keem Siah and Yap, Hwa Jen},
Title = {{A Constrained Optimization based Extreme Learning Machine for noisy data
   regression}},
Journal = {{NEUROCOMPUTING}},
Year = {{2016}},
Volume = {{171}},
Pages = {{1431-1443}},
Month = {{JAN 1}},
Abstract = {{Most of the existing Artificial Intelligence (AI) models for data
   regression commonly assume that the data samples are completely clean
   without noise or worst yet, only the symmetrical noise is in
   considerations. However in the real world applications, this is often
   not the case. This paper addresses a significant note of inefficiency in
   methods for regression when dealing with outliers, especially for cases
   with polarity of noise involved (i.e., one sided noise with either only
   positive noise or negative noise). Using soft margin loss function
   concept, we propose Constrained Optimization method based Extreme
   Learning Machine for Regression, hereafter denoted as CO-ELM-R. The
   proposed method incorporates the two Lagrange multipliers that mimic
   Support Vector Regression (SVR) into the basis of ELM to cope with
   infeasible constraints of the regression optimization problem. Thus,
   CO-ELM-R will complement the recursive iterations of SVR in the training
   phase due to the fact that ELM is much simpler in structure and faster
   in implementation. The proposed CO-ELM-R is evaluated empirically on a
   few benchmark data sets and a real world application of NOx gas emission
   data set collected from one of the power plant in Malaysia. The obtained
   results have demonstrated its validity and efficacy in handling noisy
   data regression problems. (C) 2015 Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Yap, KS (Reprint Author), Univ Tenaga Nas, Coll Grad Studies, Jalan IKRAM UNITEN, Kajang 43009, Selangor, Malaysia.
   Wong, Shen Yuong; Yap, Keem Siah, Univ Tenaga Nas, Coll Grad Studies, Kajang 43009, Selangor, Malaysia.
