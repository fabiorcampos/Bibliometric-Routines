@article{ ISI:000390734300027,
Author = {Raja, Muhammad Asif Zahoor and Niazi, Shahab Ahmad and Butt, Saeed Ahmad},
Title = {{An intelligent computing technique to analyze the vibrational dynamics
   of rotating electrical machine}},
Journal = {{NEUROCOMPUTING}},
Year = {{2017}},
Volume = {{219}},
Pages = {{280-299}},
Month = {{JAN 5}},
Abstract = {{In this study, bio-inspired computational intelligence is exploited to
   analyze the nonlinear vibrational dynamics of rotating electrical
   machine (VD-REM) model by applying artificial neural networks (ANN5),
   genetic algorithms (GAs) and active-set methods (ASMs). The
   superintended mathematical relation of VD-REM is modeled with ANN5 by
   employing an unsupervised error function. Design parameters of the
   networks are trained with meta-heuristic approach based on GAs, used as
   a tool for effective global search method, hybrid with ASM for efficient
   local search. The design scheme is evaluated for VD-REM models by taking
   different values of shaft stiffness along with an amplitude of force and
   parametric excitations. The performance of the proposed scheme is
   validated through the comparison of results from Adam numerical method
   with the help of different measures based on mean absolute derivation,
   root mean square error, and Nash-Sutcliffe efficiency. (C) 2016 Elsevier
   B.V. All rights reserved.}},
