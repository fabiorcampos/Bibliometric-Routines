@article{ ISI:000395368800011,
Author = {Almadani, Basem and Abudalfa, Shadi and Yang, Shuang-Hua},
Title = {{QoS Adaptation for Publish/Subscribe Middleware in eal-Time Dynamic
   Environments}},
Journal = {{INTERNATIONAL ARAB JOURNAL OF INFORMATION TECHNOLOGY}},
Year = {{2017}},
Volume = {{14}},
Number = {{2}},
Pages = {{230-238}},
Month = {{MAR}},
Abstract = {{Automatic Quality of Service (QoS) adaptation is promising approaches in
   developing real-time systems built on publish/subscribe middleware, and
   its importance increased when developing huge real-time distributed
   systems in dynamic environments. In this paper we proposed a new
   approach by building a closed loop QoS adaptive control for adapting
   some QoS polices automatically in publish/subscribe middleware. We use
