@article{ ISI:000321566800012,
Author = {Zang, Liang-Jun and Cao, Cong and Cao, Ya-Nan and Wu, Yu-Ming and Cao,
   Cun-Gen},
Title = {{A Survey of Commonsense Knowledge Acquisition}},
Journal = {{JOURNAL OF COMPUTER SCIENCE AND TECHNOLOGY}},
Year = {{2013}},
Volume = {{28}},
Number = {{4}},
Pages = {{689-719}},
Month = {{JUL}},
Abstract = {{Collecting massive commonsense knowledge (CSK) for commonsense reasoning
   has been a long time standing challenge within artificial intelligence
   research. Numerous methods and systems for acquiring CSK have been
   developed to overcome the knowledge acquisition bottleneck. Although
   some specific commonsense reasoning tasks have been presented to allow
   researchers to measure and compare the performance of their CSK systems,
   we compare them at a higher level from the following aspects: CSK
   acquisition task (what CSK is acquired from where), technique used (how
   can CSK be acquired), and CSK evaluation methods (how to evaluate the
   acquired CSK). In this survey, we first present a categorization of CSK
   acquisition systems and the great challenges in the field. Then, we
   review and compare the CSK acquisition systems in detail. Finally, we
   conclude the current progress in this field and explore some promising
   future research issues.}},
