@article{ ISI:000319397200002,
Author = {Zou, Jianmin and Gundry, Stephen and Kusyk, Janusz and Uyar, M. Uemit
   and Sahin, Cem Safak},
Title = {{3D genetic algorithms for underwater sensor networks}},
Journal = {{INTERNATIONAL JOURNAL OF AD HOC AND UBIQUITOUS COMPUTING}},
Year = {{2013}},
Volume = {{13}},
Number = {{1}},
Pages = {{10-22}},
Abstract = {{We introduce a genetic algorithm-based topology control mechanism, named
   3D-GA, for Autonomous Underwater Vehicles (AUVs) operating in Underwater
   Sensor Networks (UWSNs). Using limited information collected from a
   node's local neighbours, 3D-GA runs autonomously at each AUV and
   provides guidance for its speed and direction towards a uniform spatial
   distribution while maintaining network connectivity. Imprecise and
   limited neighbourhood knowledge could potentially disrupt convergence
   towards a uniform and stable spatial coverage. We demonstrate that AUVs
   running our 3D-GA create a highly resilient network that can adapt to
   changing conditions such as the addition, loss or malfunction of number
   of AUVs. We also show that the ambiguity in detecting neighbours' exact
   locations does not prevent 3D-GA from achieving a uniform coverage but
   requiring AUVs travel longer distances to stabilise. Our simulation
   software results verify that 3D-GA is an effective tool for providing a
   robust solution for volumetric spatial control of AUVs in UWSNs.}},
