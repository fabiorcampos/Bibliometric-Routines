@article{ ISI:000397939200027,
Author = {Hu Zhiyu and Li Li},
Title = {{A Study on Multi-core Task Scheduling Algorithm based on Artificial
   Intelligence}},
Journal = {{INTERNATIONAL JOURNAL OF GRID AND DISTRIBUTED COMPUTING}},
Year = {{2016}},
Volume = {{9}},
Number = {{12}},
Pages = {{307-320}},
Abstract = {{With the rapid development of science technology, the multi-core
   processor system has been become one of the hottest issues in the high
   performance computation field at present. At the same time, there are
   some problems in the application and the process of development. In
   order to find the more efficient task scheduling algorithm, this paper
   will research the multi-core processors task scheduling algorithm. With
   exploring the existing task scheduling algorithm principle, the
   heterogeneous multi-core processor system task scheduling mathematical
   model is be built, and based on the genetic algorithm, the paper
   proposes the heterogeneous multi-core processor system scheduling based
   on population genetic algorithm. Then, through the feasibility,
   parameter analysis, verification algorithm, the improved genetic
   algorithm effectively improves the system performance, and reduces the
   running time. The model, in a certain extent, increases the application
   and development of artificial intelligence, and provides a theoretical
