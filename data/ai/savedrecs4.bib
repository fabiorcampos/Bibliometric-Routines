@article{ ISI:000335905000011,
Author = {Huang, Shuiqing and Yang, Bo and Yan, Sulan and Rousseau, Ronald},
Title = {{Institution name disambiguation for research assessment}},
Journal = {{SCIENTOMETRICS}},
