@article{ ISI:000375030000017,
Author = {Edelen, A. L. and Biedron, S. G. and Chase, B. E. and Edstrom, Jr., D.
   and Milton, S. V. and Stabile, P.},
Title = {{Neural Networks for Modeling and Control of Particle Accelerators}},
