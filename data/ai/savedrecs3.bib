@article{ ISI:000348496500009,
Author = {Burns, Kevin},
Title = {{Computing the creativeness of amusing advertisements: A Bayesian model
   of Burma-Shave's muse}},
Journal = {{AI EDAM-ARTIFICIAL INTELLIGENCE FOR ENGINEERING DESIGN ANALYSIS AND
   MANUFACTURING}},
Year = {{2015}},
Volume = {{29}},
Number = {{1}},
Pages = {{109-128}},
Month = {{FEB}},
Abstract = {{How do humans judge the creativeness of an artwork or other artifact?
   This article suggests that such judgments are based on the pleasures of
   an aesthetic experience, which can be modeled as a mathematical product
   of psychological arousal and appraisal. The arousal stems from surprise,
   and is computed as a marginal entropy using information theory. The
   appraisal assigns meaning, by which the surprise is resolved, and is
   computed as a posterior probability using Bayesian theory. This model is
   tested by obtaining human ratings of surprise, meaning, and creativeness
   for artifacts in a domain of advertising design. The empirical results
   show that humans do judge creativeness as a product of surprise and
   meaning, consistent with the computational model of arousal and
   appraisal. Implications of the model are discussed with respect to
   advancing artificial intelligence in the arts as well as improving the
   computational evaluation of creativity in engineering and design.}},
