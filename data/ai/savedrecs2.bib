@article{ ISI:000380117500021,
Author = {Yu, Lean and Hu, Lunchao and Tang, Ling},
Title = {{Stock Selection with a Novel Sigmoid-Based Mixed Discrete-Continuous
   Differential Evolution Algorithm}},
