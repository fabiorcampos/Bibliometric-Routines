@article{ ISI:000415070700002,
Author = {Duggan, Martin and Duggan, Jim and Howley, Enda and Barrett, Enda},
Title = {{A reinforcement learning approach for the scheduling of live migration
   from under utilised hosts}},
Journal = {{MEMETIC COMPUTING}},
Year = {{2017}},
Volume = {{9}},
Number = {{4}},
Pages = {{283-293}},
Month = {{DEC}},
Abstract = {{Live virtual machine migration can have a major impact on how a cloud
   system performs, as it consumes significant amount of network resources,
   such as bandwidth. A virtual machine migration occurs when a host
   becomes over-utilised or under-utilised. In this paper, we propose a
   network aware live migration strategy that monitors the current demand
   level of bandwidth when network congestion occurs and performs
   appropriate actions based on what it is experiencing. The Artificial
   Intelligence technique that is based on Reinforcement Learning acts as a
   decision support system, enabling an agent to learn an optimal time to
   schedule a virtual machine migration depending on the current bandwidth
   usage in a data centre. We show from our results that an autonomous
   agent can learn to utilise available network resources such as bandwidth
   when network saturation occurs at peak times.}},
