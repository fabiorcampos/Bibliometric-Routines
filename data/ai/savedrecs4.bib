@article{ ISI:000340584700013,
Author = {Tsai, Hsing-Chih},
Title = {{Integrating artificial bee colony and bees algorithm for solving
   numerical function optimization}},
Journal = {{NEURAL COMPUTING \& APPLICATIONS}},
Year = {{2014}},
Volume = {{25}},
Number = {{3-4}},
Pages = {{635-651}},
Month = {{SEP}},
Abstract = {{This work integrates artificial bee colony (ABC) and bees algorithm (BA)
   to develop a two bees (TB) algorithm. Agents of TB are stochastically
   assigned to ABC and BA sub-swarms in each iteration according to their
   fitness values. Consequently, the current healthier sub-swarm gains more
   agents to carry out its actions. Sub-swarm populations therefore vary
   ceaselessly during iterations, while either the ABC or BA sub-swarms may
   be superior to the other. Experiments are conducted on 23 benchmark
   functions. Results demonstrate that the TB performs better than or close
   to the ABC or BA winner. TB overcomes the poor performance of ABC and BA
   in handling particular problems.}},
