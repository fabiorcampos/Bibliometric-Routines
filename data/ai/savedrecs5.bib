@article{ ISI:000314829800004,
Author = {Kulich, Miroslav and Chudoba, Jan and Kosnar, Karel and Krajnik, Tomas
   and Faigl, Jan and Preucil, Libor},
Title = {{SyRoTek-Distance Teaching of Mobile Robotics}},
Journal = {{IEEE TRANSACTIONS ON EDUCATION}},
Year = {{2013}},
Volume = {{56}},
Number = {{1, SI}},
Pages = {{18-23}},
Month = {{FEB}},
Abstract = {{E-learning is a modern and effective approach for training in various
   areas and at different levels of education. This paper gives an overview
   of SyRoTek, an e-learning platform for mobile robotics, artificial
   intelligence, control engineering, and related domains. SyRoTek provides
   remote access to a set of fully autonomous mobile robots placed in a
   restricted area with dynamically reconfigurable obstacles, which enables
   solving a huge variety of problems. A user is able to control the robots
   in real time by their own developed algorithms as well as being able to
   analyze gathered data and observe activity of the robots by provided
   interfaces. The system is currently used for education at the Czech
