@article{ ISI:000413898400009,
Author = {Kidholm, Kristian and Clemensen, Jane and Caffery, Liam J. and Smith,
   Anthony C.},
Title = {{The Model for Assessment of Telemedicine (MAST): A scoping review of
   empirical studies}},
Journal = {{JOURNAL OF TELEMEDICINE AND TELECARE}},
Year = {{2017}},
Volume = {{23}},
Number = {{9}},
Pages = {{803-813}},
Month = {{OCT}},
Abstract = {{The evaluation of telemedicine can be achieved using different
   evaluation models or theoretical frameworks. This paper presents a
   scoping review of published studies which have applied the Model for
   Assessment of Telemedicine (MAST). MAST includes pre-implementation
   assessment (e.g. by use of participatory design), followed by
   multidisciplinary assessment, including description of the patients and
   the application and assessment of safety, clinical effectiveness,
   patient perspectives, economic aspects organisational aspects and
   socio-cultural, legal and ethical aspects. Twenty-two studies met the
   inclusion criteria and were included in the review. In this article,
   research design and methods used in the multidisciplinary assessment are
   described, strengths and weaknesses are analysed, and recommendations
   for future research are presented.}},
