@article{ ISI:000343203200019,
Author = {Bellodi, Elena and Lamma, Evelina and Riguzzi, Fabrizio and Costa, Vitor
   Santos and Zese, Riccardo},
Title = {{Lifted Variable Elimination for Probabilistic Logic Programming}},
Journal = {{THEORY AND PRACTICE OF LOGIC PROGRAMMING}},
