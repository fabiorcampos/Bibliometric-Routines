@article{ ISI:000419121000018,
Author = {Hwang, Dennis and Chang, Jeremiah W. and Benjafield, Adam V. and
   Crocker, Maureen E. and Kelly, Colleen and Becker, Kendra A. and Kim,
   Joseph B. and Woodrum, Rosa R. and Liang, Joanne and Derose, Stephen F.},
Title = {{Effect of Telemedicine Education and Telemonitoring on Continuous
