@article{ ISI:000326113700056,
Author = {Leitao, Paulo and Marik, Vladimir and Vrba, Pavel},
Title = {{Past, Present, and Future of Industrial Agent Applications}},
Journal = {{IEEE TRANSACTIONS ON INDUSTRIAL INFORMATICS}},
Year = {{2013}},
Volume = {{9}},
